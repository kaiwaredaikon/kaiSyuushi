>

    <!-- 単位 -->
	<PreferenceCategory
	    android:title="@string/pref_category_01"
	>

		<ListPreference
		   android:key="@string/pref_key_tani"
           android:title="@string/pref_title_01"
           android:summary=""
           android:entries="@array/entries_tani"
           android:entryValues="@array/entryvalues_tani"
           android:dialogTitle="@string/pref_title_01"
		   android:defaultValue="500"
        />
	</PreferenceCategory>


   	<PreferenceCategory android:title="@string/pref_category_02">

		<ListPreference
           android:key="@string/pref_key_destination"
           android:title="@string/pref_title_06"
